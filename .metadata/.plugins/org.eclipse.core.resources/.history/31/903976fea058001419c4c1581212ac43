package rubik;

public class CubeNode {

	/**
	 * The state of the cube
	 */

	public char[] state;
	/**
	 * The heuristic value
	 */
	public int heuristic;

	/**
	 *
	 * @param state the state of the cube
	 * @param heuristic the heuristic value
	 */
	public CubeNode(char[] state, int heuristic) {
		this.state = state;
		this.heuristic = heuristic;
	}



	
}
